import java.util.*;
class Square{
	public static void main(String args[]){
		int a,b,c;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the lower range=");
		a = s.nextInt();
		System.out.println("Enter the upper range=");
		b =s.nextInt();
		for(int i=a;i<=b;i++)
		{
			c = i*i;
			System.out.println(i+","+c);
		}
	}
}		
